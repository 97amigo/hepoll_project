}



/*Responsive layout*/
@media screen and (max-width: 1280px) {
  .nav {
    width: 70%;
  }
  .column-certs {
    width: 50%;
  }
  .column-command {
    width: 50%;
  }
  .contacts {
    display: none;
  }
}

@media screen and (max-width: 1024px) {
  .nav {
    display: none;
  }

  .menu__btn {
    visibility: visible;
  }

  .main-container--btns {
    display: none;
  }

  .main-container--material {
    padding: 0;
    width: 100%;
  }

  .logo-green a img{
      content: url("/img/logo_footer_mob.svg");
  }

  .column-company {
    width: 100%;
  }

  #row-reqs {
    margin: 0;
  }
}

@media screen and (max-width: 800px) {
  html {
    font-size: 15px;
  }

  .column-command {
    width: 100%;
  }
} 

@media screen and (max-width: 640px) {
  html {
    font-size: 14px;
  }

  .dropbtn {
    padding-left: 50px;
    padding-right: 30px;
  }

  #simple-btn {
    padding-left: 50px;  
  }

  #row-reqs {
    padding: 10px;
  }

  .column-certs {
    width: 100%;
  }
}

@media screen and (max-width: 412px) {
  html {
    font-size: 13px;
  }

  .wrapper {
    margin: 0px 20px 0 20px;
  }

  .menu__btn {
    right: 20px;
  }

  .dropdown {
    margin: 0 20px;
  }

  .dropbtn {
    padding-left: 20px;
    padding-right: 15px;
  }

  #simple-btn {
    padding-left: 20px;  
  }

  .column-company {
    padding: 10px 10px 10px 0;
  }

  #row-reqs, 
  #row-reqs td:first-child {
    font-size: 1rem;
  }

}

@media screen and (max-width: 320px) {
  html {
    font-size: 12px;
  }

  .column-company img {
    height: 400px;
  }
}